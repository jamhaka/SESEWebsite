@page
@model SESEWebsite.Pages.Students.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Register</a>
</p>
<table class="table">
    <thead>
        <tr>
            
            <th>
                @Html.DisplayNameFor(model => model.Register[0].FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Register[0].LastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Register[0].OtherName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Register[0].FullName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Register[0].DOB)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Register[0].PhoneNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Register[0].Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Register[0].Password)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Register[0].ConfirmPassword)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Register) {
        <tr>
            
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OtherName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FullName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DOB)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PhoneNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ConfirmPassword)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
